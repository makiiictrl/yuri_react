- var theme_customizer      = true;
- var chartist              = true;
- var page_chartist         = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var script2               = true;
- var scrollbar             = true;

//- mixin
include ../../mixins/mixin

doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      .page-header.row
        include ../../components/header
        +left-header
        +breadcrumb(['Charts','Chartist Chart'],'Chartist Chart')
        +right-header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          // Container-fluid starts
          .container-fluid
            .row
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Advanced SMIL Animations
                  .card-body
                    .ct-6.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 SVG Path Animation
                  .card-body
                    .ct-7.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Animating a Donut with Svg.animate
                  .card-body
                    .ct-8.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Bi-polar Line Chart with Area only
                  .card-body
                    .ct-5.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Line Chart with Area
                  .card-body
                    .ct-4.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Bi-polar Bar Chart
                  .card-body
                    .ct-9.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Stacked Bar Chart
                  .card-body
                    .ct-10.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Horizontal Bar Chart
                  .card-body
                    .ct-11.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Extreme Responsive Configuration
                  .card-body
                    .ct-12.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Simple Line Chart
                  .card-body
                    .ct-1.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Holes in Data
                  .card-body
                    .ct-2.flot-chart-container
              .col-xl-6.col-md-12.col-sm-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Filled Holes in Data
                  .card-body
                    .ct-3.flot-chart-container
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
