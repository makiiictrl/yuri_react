- var theme_customizer      = true;
- var chartjs               = true;
- var touchspin_2           = true;
- var page_chartjs          = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
- var script2               = true;

//- mixin
include ../../mixins/mixin

doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      .page-header.row
        include ../../components/header
        +left-header
        +breadcrumb(['Charts','ChartJS Char'],'ChartJS Char')
        +right-header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          // Container-fluid starts
          .container-fluid
            .row
              .col-xl-6.col-md-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Bar Chart
                  .card-body.chart-block
                    canvas#myBarGraph
              .col-xl-6.col-md-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Line Graph
                  .card-body.chart-block
                    canvas#myGraph
              .col-xl-6.col-md-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Radar Graph
                  .card-body.chart-block
                    canvas#myRadarGraph
              .col-xl-6.col-md-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Line Chart
                  .card-body.chart-block
                    canvas#myLineCharts
              .col-xl-6.col-md-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Doughnut Chart
                  .card-body.chart-block.chart-vertical-center
                    canvas#myDoughnutGraph
              .col-xl-6.col-md-12.box-col-12
                .card.title-line
                  .card-header
                    h2 Polar Chart
                  .card-body.chart-block.chart-vertical-center
                    canvas#myPolarGraph
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
