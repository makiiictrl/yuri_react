- var theme_customizer      = true;
- var vector_map            = true;
- var page_vector_map       = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var script2               = true;
- var scrollbar             = true;

//- mixin
include ../../mixins/mixin

doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      .page-header.row
        include ../../components/header
        +left-header
        +breadcrumb(['Maps','Vector Maps'],'Vector Maps')
        +right-header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          // Container-fluid starts
          .container-fluid
            .row
              .col-sm-6
                .card
                  .card-header
                    h2 Vector World Map
                    p.f-m-light.mt-1 Below Map is displaying the world map.
                  .card-body
                    #world-map.jvector-map-height
              .col-sm-6
                .card
                  .card-header
                    h2 Vector Usa Map
                    p.f-m-light.mt-1 Below Map is displaying the usa map.
                  .card-body
                    #usa.jvector-map-height
              .col-sm-6
                .card
                  .card-header
                    h2 Vector Canada Map
                    p.f-m-light.mt-1 Below Map is displaying the canada map.
                  .card-body
                    #canada.jvector-map-height
              .col-sm-6
                .card
                  .card-header
                    h2 Vector India Map
                    p.f-m-light.mt-1 Below Map is displaying the india map.
                  .card-body
                    #india.jvector-map-height
              .col-sm-6
                .card
                  .card-header
                    h2 Vector Asia Map
                    p.f-m-light.mt-1 Below Map is displaying the asia map.
                  .card-body
                    #asia.jvector-map-height
              .col-sm-6
                .card
                  .card-header
                    h2 Vector Canada Map
                    p.f-m-light.mt-1 Below Map is displaying the uk map.
                  .card-body
                    #uk.jvector-map-height
              .col-sm-6
                .card
                  .card-header
                    h2 Vector Chicago Map
                    p.f-m-light.mt-1 Below Map is displaying the chicago map.
                  .card-body
                    #chicago.jvector-map-height
              .col-sm-6
                .card
                  .card-header
                    h2 Vector Australia Map
                    p.f-m-light.mt-1 Below Map is displaying the australia map.
                  .card-body
                    #australia.jvector-map-height

          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
