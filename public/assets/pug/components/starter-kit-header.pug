- var status_toggle = (sidebar_open ? undefined : 'checked');
// Page Header Start

mixin left-header
  .col-auto.header-left-wrapper
    .header-logo-wrapper.p-0.left-header
        .logo-wrapper
          a(href='index.html')
            img.img-fluid(src='../assets/images/logo/logo_dark.png', alt='')
            
    .toggle-sidebar
        svg.status_toggle.sidebar-toggle
          use(href='../assets/svg/icon-sprite.svg#collapse-sidebar')

mixin right-header
  .col.header-wrapper.m-0.header-right-wrapper
    .row.m-0
      form(action='#', method='get').form-inline.search-full.col
        .form-group.w-100
          .Typeahead.Typeahead--twitterUsers
            .u-posRelative
              input.demo-input.Typeahead-input.form-control-plaintext.w-100(type='text' placeholder='Search anything ..' name='q' title='' autofocus)
              .spinner-border.Typeahead-spinner(role='status')
                span.sr-only Loading...
              i(data-feather='x').close-search
            .Typeahead-menu
      .header-logo-wrapper.col-auto.p-0.left-header
      .nav-right.col-auto.pull-right.right-header.p-0.ms-auto
        ul.nav-menus
          li                          
            span.header-search
              svg
                use(href='../assets/svg/icon-sprite.svg#search')
          li.onhover-dropdown
            .notification-box.onhover-click
              svg
                use(href='../assets/svg/icon-sprite.svg#notification')
              span.badge.rounded-pill.badge-success 3 
            .onhover-show-div.notification-dropdown
              h6.f-18.mb-0.dropdown-title Notifications                               
              ul
                li.d-flex
                  .notification-image 
                    img.img-fluid(src="../assets/images/avtar/3.jpg", alt="user")
                    .notification-icon.bg-danger
                      i(class="fa fa-heart")
                  div
                    p
                      span.f-w-500.me-1 Emay Walter 
                      | liked your post 
                    span.f-light 10 minutes ago
                li.d-flex
                  .notification-image 
                    img.img-fluid(src="../assets/images/avtar/4.jpg", alt="user")
                    .notification-icon.bg-info
                      i(class="fa fa-share-alt")
                  div
                    p
                      span.f-w-500.me-1 Allie Grater
                      | shared your post 
                    span.f-light 5 hours ago
                li.d-flex
                  .notification-image 
                    img.img-fluid(src="../assets/images/avtar/7.jpg", alt="user")
                    .notification-icon.bg-success
                      i(class="fa fa-wechat")
                  div
                    p
                      span.f-w-500.me-1 Colin Sik
                      | commented on your post
                    span.f-light yesterday
                li
                  a.f-w-700(href="#!") Check all
          li.cart-nav.onhover-dropdown
            .cart-box.onhover-click
              svg
                use(href='../assets/svg/icon-sprite.svg#email')
              span.badge.rounded-pill.badge-danger 2
            .onhover-show-div.chat-dropdown
                .dropdown-title
                  .d-flex.align-items-center 
                    img.img-fluid.img-40.rounded-circle(src='../assets/images/dashboard/user/1.jpg',alt='user')
                    div
                      h6.f-18.mb-0 Jane Cooper
                      p.mb-0 
                        span.status.status-success.me-1
                        span active
                ul 
                  li.send-msg
                    div 
                      img.img-fluid.img-30.rounded-circle(src='../assets/images/dashboard/user/1.jpg',alt='user')
                      div 
                        p Hello.how are you?
                  li.reply-msg
                    div 
                      img.img-fluid.img-30.rounded-circle(src='../assets/images/dashboard/user/3.jpg',alt='user')
                      div 
                        p Hi, i'm fine.what about you?
                  li.send-msg
                    div 
                      img.img-fluid.img-30.rounded-circle(src='../assets/images/dashboard/user/1.jpg',alt='user')
                      div 
                        p I am facing js issue can you help me?
                  li.reply-msg
                    div 
                      img.img-fluid.img-30.rounded-circle(src='../assets/images/dashboard/user/3.jpg',alt='user')
                      div 
                        p Sure, i will help you.
                .chat-input
                  .input-group
                    input.form-control(type='text' placeholder="Type message here...")
                    span.input-group-text.bg-primary  
                      svg
                        use(href='../assets/svg/icon-sprite.svg#send')
          li
            .mode
              svg.moon-icon
                use(href='../assets/svg/icon-sprite.svg#moon')
              svg.sun-icon
                use(href='../assets/svg/icon-sprite.svg#sun')
          li.cart-nav.onhover-dropdown
            .cart-box.onhover-click
              svg
                use(href='../assets/svg/icon-sprite.svg#stroke-ecommerce')
              span.badge.rounded-pill.badge-primary 2
            .cart-dropdown.onhover-show-div
              h6.f-18.mb-0.dropdown-title Cart
              ul.cart-main-wrapper
                li.cart-product
                  .d-flex
                    img.img-fluid.b-r-5.me-3.img-60(src='../assets/images/other-images/cart-img2.jpg', alt='cosmetic')
                    .flex-grow-1
                      span.f-w-500 Beauty cosmetic
                      .qty-box
                        .touchspin-wrapper 
                            button.decrement-touchspin.btn-touchspin
                                i.fa.fa-minus.text-gray
                            input#inputData.input-touchspin(type='number' value='2')
                            button.increment-touchspin.btn-touchspin
                                i.fa.fa-plus.text-gray
                        h6.font-primary $20        
                      
                    .close-circle
                      a(href='#')
                        i(data-feather='trash-2')
                li.cart-product
                  .d-flex
                    img.img-fluid.b-r-5.me-3.img-60(src='../assets/images/other-images/cart-img-1.jpg', alt='shoes')
                    .flex-grow-1
                      span.f-w-500 Simple shoes
                      .qty-box
                        .touchspin-wrapper 
                            button.decrement-touchspin.btn-touchspin
                                i.fa.fa-minus.text-gray
                            input#inputData1.input-touchspin(type='number' value='1')
                            button.increment-touchspin.btn-touchspin
                                i.fa.fa-plus.text-gray
                        h6.font-primary $30        
                      
                    .close-circle
                      a(href='#')
                        i(data-feather='trash-2')
                li.total
                  h6.mb-0
                    | Order Total : 
                    span.f-right $70.00
                li.text-center
                  a.d-block.mb-3.view-cart.f-w-700(href='../template/cart.html')
                    | Go to your cart
                  a.btn.btn-primary.view-checkout.w-100(href='../template/checkout.html')
                    | Checkout
              .cart-empty
                .cart-image 
                  img.img-fluid(src='../assets/images/product/order-trash.gif' alt='empty')
                h5 Oops! Your cart empty!!!
          li.profile-nav.onhover-dropdown
            .onhover-click
              .sidebar-image 
                img(src="../assets/images/user.png", alt="profile")
                span.status.status-success
              div.sidebar-content 
                h4 Wade Warren
                span.f-12.f-w-600.f-light UI Designer
            ul.profile-dropdown.onhover-show-div
              li
                a(href='../template/sign-up.html')
                  .profile-icon
                    svg
                      use(href='../assets/svg/icon-sprite.svg#user')
                  span Account 
              li
                a(href='../template/letter-box.html')
                  .profile-icon
                    svg
                      use(href='../assets/svg/icon-sprite.svg#stroke-email')
                  span Inbox
              li
                a(href='../template/task.html')
                  .profile-icon
                    svg
                      use(href='../assets/svg/icon-sprite.svg#notepad')
                  span Taskboard
              li
                a(href='../template/edit-profile.html')
                  .profile-icon
                    svg
                      use(href='../assets/svg/icon-sprite.svg#settings')
                  span Settings
              li
                a(href='../template/login.html')
                  .profile-icon
                    svg
                      use(href='../assets/svg/icon-sprite.svg#login')
                  span Log out
      //- .d-lg-none.mobile-toggle.pull-right
      //-     i(data-feather='more-horizontal')
      script.result-template(type='text/x-handlebars-template').
        <div class="ProfileCard u-cf">                        
        <div class="ProfileCard-avatar"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-airplay m-0"><path d="M5 17H4a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h16a2 2 0 0 1 2 2v10a2 2 0 0 1-2 2h-1"></path><polygon points="12 15 17 21 7 21 12 15"></polygon></svg></div>
        <div class="ProfileCard-details">
        <div class="ProfileCard-realName">{{name}}</div>
        </div>
        </div>
      script.empty-template(type='text/x-handlebars-template').
        <div class="EmptyMessage">Your search turned up 0 results. This most likely means the backend is down, yikes!</div>
  // Page Header Ends